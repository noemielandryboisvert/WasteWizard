{
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "_env": true
    },
    "parser": "babel-eslint",
    "rules": {
        "class-methods-use-this": 0,
        "curly": 0,
        "func-names": 0,
        "linebreak-style": 0,
        "object-shorthand": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "space-before-function-paren": 0
    }
}